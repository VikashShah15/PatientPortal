    <script>
        //Custom design form example
        $(".tab-wizard").steps({
            headerTag: "h6",
            bodyTag: "section",
            transitionEffect: "fade",
            titleTemplate: '<span class="step">#index#</span> #title#',
            labels: {
                finish: "YES, SCHEDULE THIS APPOINTMENT"
            },
            onFinished: function (event, currentIndex) {
                Swal.fire("Live Appointment Success!", "Your Appointment has successfully been scheduled for Friday, July 31 2020 10:30 AM with REED FOWLER at Patient Portal - Main Office.");
                setTimeout(function () { window.location = "MyAppointments.aspx"; }, 4000);

            }
        });

        $(".validation-wizard").steps({
            headerTag: "h6",
            bodyTag: "section",
            transitionEffect: "fade",
            titleTemplate: '<span class="step">#index#</span> #title#',
            labels: {
                finish: "SUBMIT"
            },
            onFinished: function (event, currentIndex) {
                //Swal.fire("Live Appointment Success!", "Your Appointment has successfully been scheduled for Friday, July 31 2020 10:30 AM with REED FOWLER at Patient Portal - Main Office.");
                setTimeout(function () { window.location = "Dashboard.aspx"; }, 1000);

            }
        });

        $('#example23').DataTable({
            dom: 'Bfrtip',
            responsive: true
        });

        $('#myTab a').click(function (e) {
            e.preventDefault();
            $(this).tab('show');
        });

        $('#mychartmenu a').click(function (e) {
            $(this).tab('show');
            //window.location.reload();
        });

        // store the currently selected tab in the hash value
        $("ul.test > li > a").on("shown.bs.tab", function (e) {
            var id = $(e.target).attr("href");
            window.location.hash = id;
            window.location.href = id;
            window.location.reload();
        });

        $(document).ready(function () {
            // on load of the page: switch to the currently selected tab
            var hash = window.location.hash;
            $('#myTab a[href="' + hash + '"]').tab('show');
        });
    </script>
